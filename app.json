{
  "expo": {
    "name": "Notitas",
    "slug": "tus-notitas",
    "version": "1.0.0",
    "scheme": "notitas",
    "orientation": "portrait",
    "icon": "./src/assets/notitas-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/notitas-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/notitas-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.louisrubin.tusnotitas"
    },
    "web": {
      "favicon": "./src/assets/notitas-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7b967558-ef4d-495f-b1f4-c72bdbad8322"
      }
    }
  }
}
